{
  "engines": {
    "node": "16.x.x"
  },
  "name": "beehive",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "tsc --project tsconfig.json",
    "package": "cp -r package.json ./lib",
    "css": "mkdirp ./lib/css && cp -r ./css/* ./lib/css",
    "images": "mkdirp ./lib/images && cp -r images/* ./lib/images",
    "video": "mkdirp ./lib/video && cp -r video/* ./lib/video",
    "html": "cp *.html ./lib/",
    "favicon": "mkdirp ./lib/favicon && cp -r favicon/* ./lib/favicon",
    "hbs": "mkdirp ./lib/render-templates && cp -r render-templates/* ./lib/render-templates",
    "start": "node ./beehive-server.js",
    "gcp-build": "npm run build-target",
    "delete": "rm -rf lib",
    "build": "npm run delete && npm run compile && npm run package && npm run css && npm run images && npm run video && npm run html && npm run hbs && npm start",
    "watch-html": "npm run compile && node ./lib/html-file-watch.js",
    "upload-css": "gsutil -h\"Cache-Control:no-cache\" -h\"Content-Encoding: gzip\" cp -z css ./css/*.css gs://cap20",
    "upload-images": "gsutil -h\"Cache-Control:no-cache\" cp ./images/*.* gs://cap20",
    "cors": "gsutil cors set cors.json gs://cap20",
    "build-target": "npm run compile"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/logging": "^9.5.5",
    "@google-cloud/secret-manager": "^3.10.0",
    "connect-mongo": "^4.4.1",
    "connect-mongodb-session": "^3.0.0",
    "crypto": "^1.0.1",
    "express": "^4.17.1",
    "express-handlebars": "^5.3.3",
    "express-handlebars-sections": "^1.0.1",
    "express-session": "^1.17.2",
    "handlebars": "^4.7.7",
    "isomorphic-dompurify": "^0.15.0",
    "mkdirp": "^1.0.4",
    "mongodb": "^4.1.1",
    "mongoose": "^5.13.7",
    "nodemailer": "^6.6.5",
    "passport": "^0.4.1",
    "passport-google-oauth": "^2.0.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "systemjs": "^6.10.3",
    "tslib": "^2.3.1",
    "typescript": "^4.3.5"
  },
  "type": "module",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/express-handlebars": "^5.3.1",
    "@types/express-session": "^1.17.4",
    "@types/passport": "^1.0.7",
    "@types/passport-local": "^1.0.34"
  }
}